{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "react_app/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/react_app/dist/$1"
        }
    ]
}